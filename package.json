{
    "name": "zambra-s-snippets",
    "displayName": "Zambra's Snippets",
    "description": "Some snippets to facilitate and speed up your development.",
    "version": "0.0.33",
    "engines": {
        "vscode": "^1.80.0"
    },
    "categories": [
        "Snippets",
        "Programming Languages",
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./extension.js",
    "contributes": {
        "snippets": [
            {
                "language": "apex",
                "path": "./snippets/snippets.apex"
            },
            {
                "language": "javascript",
                "path": "./snippets/snippets.javascript"
            }
        ],
        "commands": [
            {
                "command": "zambra-s.forceapexdeploy",
                "title": "Force Apex Deploy"
            },
            {
                "command": "zambra-s.json2apex",
                "title": "Convert JSON to Apex"
            },
            {
                "command": "zambra-s.minifyText",
                "title": "Minify Text"
            }
        ]
    },
    "scripts": {
        "lint": "eslint .",
        "pretest": "npm run lint",
        "test": "node ./test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.0",
        "@types/node": "^12.11.7",
        "@types/vscode": "^1.46.0",
        "eslint": "^7.9.0",
        "glob": "^7.1.6",
        "mocha": "^8.1.3",
        "typescript": "^4.0.2",
        "vscode-test": "^1.4.0"
    },
    "keywords": [
        "Salesforce",
        "Apex",
        "Snippets"
    ],
    "publisher": "GabrielZambrin",
    "icon": "icon/sflogo.jpg",
    "author": {
        "name": "Gabriel Zambrin"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/gabrielzambrin/Zambra-Snippets"
    },
    "dependencies": {
        "jsonminify": "^0.4.2",
        "node-fetch": "^2.6.12"
    }
}
