{
    "name": "zambra-s-snippets",
    "displayName": "Zambra's Snippets",
    "description": "Some snippets to facilitate and speed up your development.",
    "version": "0.0.18",
    "engines": {
        "vscode": "^1.48.0"
    },
    "categories": [
        "Snippets",
        "Programming Languages",
        "Other"
    ],
    "activationEvents": [
        "*"
	],
	"main": "./extension.js",
    "contributes": {
        "snippets": [
            {
                "language": "apex",
                "path": "./snippets/snippets.apex"
            },
            {
                "language": "javascript",
                "path": "./snippets/snippets.javascript"
            }
        ],
        "commands": [
            {
                "command": "extension.forceapexdeploy",
                "title": "Force Apex Deploy"
		    },
            {
                "command": "extension.forceLwcDeploy",
                "title": "Force Lightning Web Component Deploy"
		    }
        ]
    },
    "scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
        "@types/vscode": "^1.46.0",
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.0.0",
        "@types/node": "^12.11.7",
        "eslint": "^7.9.0",
        "glob": "^7.1.6",
        "mocha": "^8.1.3",
        "typescript": "^4.0.2",
        "vscode-test": "^1.4.0"
    },
    "keywords": [
        "Salesforce",
        "Apex",
        "Snippets"
    ],
    "publisher": "GabrielZambrin",
    "icon": "icon/sflogo.jpg",
    "author": {
        "name": "Gabriel Zambrin"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/gabrielzambrin/Zambra-Snippets"
    }
}
